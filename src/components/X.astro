---
interface Props {
  class?: string;
  height?: number;
  width?: number;
}

const { class: clazz, height = 32, width = 32 } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  class={clazz}
  viewBox="0 0 24 24"
  aria-hidden="true"
  ><path
    fill="none"
    stroke="currentColor"
    stroke-linecap="round"
    stroke-linejoin="round"
    stroke-width="2"
    d="M18 6L6 18M6 6l12 12"></path></svg
>
